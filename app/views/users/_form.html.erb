<h3> Profile photo upload </h3>
<div id="add_photo">
<%= render("layouts/errors", :object => @user) %>
<%= form_for(@user, :html => { :multipart => true } , remote: true) do |form| %>
   <div class="form-group">
  <%= form.file_field :photo %>
  </div>
  <%= form.submit(:class => "btn btn-success") %>
<% end %><br>
<%= link_to "Cancel", :back %>
</div>